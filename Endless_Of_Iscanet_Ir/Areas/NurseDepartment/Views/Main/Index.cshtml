                    <img src="~/Content/Files/EventFiles/@Eve.FileName" alt="@Eve.Congress_Name | @Eve.Author" class="card-img-top w-100 d-block" itemprop="photo" />
                    <figcaption class=" d-flex justify-content-center card-title pb-0" itemprop="about"><h2 class="text-black-50 head-line-cards">@Eve.Congress_Name</h2></figcaption>
                </figure>
                <section class="card-body" itemprop="text">
                    <p class="card-text text-black-50 text-truncate d-block txt-trnc">
                        @Eve.Congress_Abstract
                    </p>
                </section>
                <div class="card-footer bg-transparent">
                    @*@Html.ActionLink(Resources.ButtonResources.Details, "Details", "Articles", new { id = LastNewss.LastNewsId, @class = "btn btn-outline-success d-block w-100" })*@
                    @Html.ActionLink(Resources.ButtonResources.Details, "Details", "Events", new { id = Eve.NurseDepCongressId ,Area="NurseDepartment" }, new { @class = "btn btn-outline-success d-block btn-round" })
                </div>
            </div>
        }
    </section>

</article>
