<div class="container-fluid">
    <div class="bd-example">
        <div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
                <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <figure class="carousel-item active">
                    <img src="~/Content/Images/Course2.jpg" class="d-block w-100" alt="@Resources.LabelResources.NurseDepartment @Resources.LabelResources.Of  @Resources.LabelResources.Iranian_socity_of_cardiac_ansthesiology">
                    <figcaption class="carousel-caption d-none d-md-block">
                        <h5>@Resources.LabelResources.Iranian_socity_of_cardiac_ansthesiology</h5>
                        <p>Nulla vitae elit libero, a pharetra augue mollis interdum.</p>
                    </figcaption>
                </figure>
                <figure class="carousel-item">
                    <img src="~/Content/Images/NurseDepartment_News2.jpg" class="d-block w-100" alt="@Resources.LabelResources.Iranian_socity_of_cardiac_ansthesiology">
                    <figcaption class="carousel-caption d-none d-md-block text-black-50">
                        <h5>@Resources.LabelResources.NurseDepartment</h5>
                        <p>
                            @Resources.LabelResources.NurseDepartment @Resources.LabelResources.Of  @Resources.LabelResources.Iranian_socity_of_cardiac_ansthesiology
                            <a href="/NurseDepartment/Main/Index">@Resources.LabelResources.NurseDepartment</a>
                        </p>
                    </figcaption>
                </figure>
                <div class="carousel-item">
                    <img src="~/Content/Images/NurseDepartment_Books2.jpg" class="d-block w-100" alt="@Resources.LabelResources.NurseDepartment @Resources.LabelResources.Of @Resources.LabelResources.Iranian_socity_of_cardiac_ansthesiology">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Third slide label</h5>
                        <p>Praesent commodo cursus magna, vel scelerisque nisl consectetur.</p>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>



<article class="row">
    <section class="container ">

        <section class="col-12">
            <a class="txt-dcr" href="/LastNews/Index">
                <h2 class=" text-dark mb-4 pb-2 text-center">
                    @Resources.LabelResources.LastNews
                </h2>
            </a>
        </section>

        @foreach (var LastNewss in Model.News)
            {

            <div class="card col-lg-4 col-md-12 Shadow" itemscope itemtype="http://schema.org/BlogPosting">

                <figure class="card-header" itemscope itemtype="http://schema.org/Photograph">
                    <img src="~/Content/Files/EventFiles/@LastNewss.FileName" alt="@LastNewss.News_Title | @LastNewss.News_Author" class="card-img-top w-100 d-block" itemprop="photo" />
                    <figcaption class=" d-flex justify-content-center card-title pb-0" itemprop="about"><h2 class="text-black-50 head-line-cards" >@LastNewss.News_Title</h2></figcaption>
                </figure>
                <section class="card-body" itemprop="text">
                    <p class="card-text text-black-50 text-truncate d-block mw-100">
                        @LastNewss.News_Abstract
                    </p>
                </section>
                <div class="card-footer bg-transparent">

                    @Html.ActionLink(Resources.ButtonResources.Details, "Details", "NurseDepartment_News", new { id = LastNewss.NurseDep_NewsId, Area = "NurseDepartment" }, new { @class = "btn btn-outline-success d-block btn-round" })


                </div>
            </div>

        }
    </section>

</article>

<article class="row">
    <section class="container ">

        <section class="col-12 "><h2 class=" text-dark display-4 mb-4 pb-2 text-center headline-font">@Resources.LabelResources.EductionCourse</h2></section>

        @foreach (var Eve in Model.EductionCources)
            {

            <div class="card col-12 col-md-4 Shadow " itemscope itemtype="http://schema.org/BlogPosting">

                <figure class="card-header-pills" itemscope itemtype="http://schema.org/Photograph">
                    <img src="~/Content/Files/EventFiles/@Eve.FileName" alt="@Eve.Title | @Eve.Author" class="card-img-top w-100 d-block" itemprop="photo" />
                    <figcaption class=" d-flex justify-content-center card-title pb-0" itemprop="about"><h2 class="text-black-50 head-line-cards">@Eve.Title</h2></figcaption>
                </figure>
                <section class="card-body" itemprop="text">
                    <p class="card-text text-black-50 text-truncate d-block txt-trnc">
                        @Eve.Abstract
                    </p>
                </section>
                <div class="card-footer bg-transparent">
                    @*@Html.ActionLink(Resources.ButtonResources.Details, "Details", "Articles", new { id = LastNewss.LastNewsId, @class = "btn btn-outline-success d-block w-100" })*@
                    @Html.ActionLink(Resources.ButtonResources.Details, "Details", "EducationCources", new { id = Eve.EductionCourcesId,Area="EducationCoures" }, new { @class = "btn btn-outline-success d-block btn-round" })
                </div>
            </div>
        }
    </section>

</article>


<article class="row">
